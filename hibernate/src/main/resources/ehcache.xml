<?xml version="1.0" encoding="UTF-8" ?>
<ehcache>
    <diskStore path="java.io.tmpdir"/>
    <defaultCache>
        <!--→缓存中最大允许保存的对象数量-->
        maxElementsInMemory="10000"
        <!--→缓存中数据是否为常量-->
        eternal="false"
        <!--→缓存数据钝化时间，单位为秒-->
        timeToIdleSeconds="120"
        <!--→缓存数据生存时间，单位为秒-->
        timeToLiveSeconds="120"
        <!--→内存不足时，是否启用磁盘缓存-->
        overflowToDisk="true"
    </defaultCache>
</ehcache>